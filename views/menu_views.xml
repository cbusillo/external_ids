<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Main Menu -->
    <menuitem
            id="menu_external_ids_root"
            name="External IDs"
            sequence="50"
            groups="base.group_system"
            web_icon="external_ids,static/description/icon.png"/>

    <!-- Configuration Menu -->
    <menuitem
            id="menu_external_ids_config"
            name="Configuration"
            parent="menu_external_ids_root"
            sequence="100"/>

    <!-- External Systems Menu -->
    <menuitem
            id="menu_external_systems"
            name="External Systems"
            parent="menu_external_ids_config"
            action="action_external_system"
            sequence="10"/>

    <!-- External IDs Menu -->
    <menuitem
            id="menu_external_ids"
            name="All External IDs"
            parent="menu_external_ids_root"
            action="action_external_id"
            sequence="10"/>

</odoo>